<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#121216</Color>
    <Color x:Key="ElementColor">#16161A</Color>
    <Color x:Key="ButtonColor">#18181D</Color>
    <Color x:Key="FocusColor">#303030</Color>
    <Color x:Key="AccentColor">#7000D0</Color>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="ElementBrush" Color="{StaticResource ElementColor}"/>
    <SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource ButtonColor}"/>
    <SolidColorBrush x:Key="FocusBrush" Color="{StaticResource FocusColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
</ResourceDictionary>